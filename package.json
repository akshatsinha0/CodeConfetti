{
  "name":"code-confetti",
  "displayName":"Code Confetti",
  "version":"0.0.1",
  "publisher":"akshatsinha0",
  "engines":{"vscode":"^1.74.0"},
  "categories":["Other"],
  "activationEvents":["onStartupFinished","onCommand:codeConfetti.trigger"],
  "main":"./out/extension.js",
  "contributes":{
    "commands":[
      {"command":"codeConfetti.trigger","title":"Code Confetti: Celebrate"}
    ]
  },
  "scripts":{
    "vscode:prepublish":"npm run compile",
    "compile":"tsc -p ./",
    "watch":"tsc -watch -p ./",
    "package":"vsce package",
    "publish":"vsce publish"
  },
  "devDependencies":{
    "@types/vscode":"^1.74.0",
    "typescript":"^5.4.0",
    "vsce":"^2.15.0"
  }
}